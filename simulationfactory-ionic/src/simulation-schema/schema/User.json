{
    "$id": "User",
    "type": "object",
    "required": ["username", "password"],
    "properties": {
        "username": {
            "type": "string",
            "minLength": 1
        },
        "password": {
            "type": "string",
            "pattern": "(?=.*[A-Z])(?=.*[a-z])(?=.*[0-9])(?=.*[^a-zA-Z0-9])",
            "maxlength": 72,
            "minLength": 6
        }
    },
    "additionalProperties": false
}